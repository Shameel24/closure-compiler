import java.util.*;
 import java.lang.*;
 import java.io.*;
 
 
 class Codechef
 {
 	public static void main (String[] args) throws java.lang.Exception
 	{
 		char a;
 		Scanner in=new Scanner(System.in);
 		a=in.next().charAt(0);
 		if(a >= 'a' && a <= 'z' || a >= 'A' && a <= 'Z')
 		System.out.println("alphabet");
 		else
 		System.out.println("not an alphabet");
 		
 	}
 }
 
