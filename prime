import java.util.*;
 
class ReverseString
{
   public static void main(String args[])
   { int a,i,flag=1;
      
      Scanner in = new Scanner(System.in);
 
      
       a= in.nextInt();
 
     
      for (  i =2; i <=a/2 ; i++ )
      {
          if(a%i==0)
     flag=0;
      }
      if(flag==0)
     System.out.println("prime");
      else
      System.out.println("not a prime");
   }
}
