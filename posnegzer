

import java.util.*;
import java.lang.*;
import java.io.*;


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int numbr;
		Scanner in=new Scanner(System.in);
		numbr=in.nextInt();
		if(numbr<0)
		System.out.println("negative");
		if(numbr>0)
		System.out.println("positive");
		if(numbr==0)
		System.out.println("zero");
	}
}
