import java.util.*;
 
class ReverseString
{
   public static void main(String args[])
   { int a,temp,sum=0,n;
      
      Scanner in = new Scanner(System.in);
 a= in.nextInt();
 temp=a;
 while(a>0)
 {
     n=a%10;
     sum=sum+(n*n*n);
     a=a/10;
 }
   if(temp==sum)
   System.out.println("amstrong number");
else
   System.out.println("not an amstrong number");
       
   }
}
