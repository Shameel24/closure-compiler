import java.util.*;
 import java.lang.*;
 import java.io.*;
 
 
 class Codechef
 {
 	public static void main (String[] args) throws java.lang.Exception
 	{
 		char a;
 		Scanner in=new Scanner(System.in);
 		a=in.next().charAt(0);
 		if(a=='a'||a=='e'||a=='i'||a=='o'||a=='u'||a=='A'||a=='E'||a=='I'||a=='O'||a=='U')
 		System.out.println("vowel");
 		else
 		System.out.println("consonant");
 		
 	}
 }
 
